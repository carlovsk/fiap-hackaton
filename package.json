  "description": "Projeto desenvolvido no Hackaton do curso 9SOAT da FIAP.",
  "license": "ISC",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "npm run lint --workspaces --if-present",
